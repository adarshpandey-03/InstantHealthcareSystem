<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>loginservlet</servlet-name>
        <servlet-class>instantHealthcare_servlets.loginservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signupservlet</servlet-name>
        <servlet-class>instantHealthcare_servlets.signupservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logoutservlet</servlet-name>
        <servlet-class>instantHealthcare_servlets.logoutservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doctorservlet</servlet-name>
        <servlet-class>instantHealthcare_servlets.doctorservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doctorOnlineAppointmentServlet</servlet-name>
        <servlet-class>instantHealthcare_servlets.doctorOnlineAppointmentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doctorOfflineAppointmentServlet</servlet-name>
        <servlet-class>instantHealthcare_servlets.doctorOfflineAppointmentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sendNotificationServlet</servlet-name>
        <servlet-class>instantHealthcare_servlets.sendNotificationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginservlet</servlet-name>
        <url-pattern>/loginservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signupservlet</servlet-name>
        <url-pattern>/signupservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logoutservlet</servlet-name>
        <url-pattern>/logoutservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doctorservlet</servlet-name>
        <url-pattern>/doctorservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doctorOnlineAppointmentServlet</servlet-name>
        <url-pattern>/doctorOnlineAppointmentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doctorOfflineAppointmentServlet</servlet-name>
        <url-pattern>/doctorOfflineAppointmentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sendNotificationServlet</servlet-name>
        <url-pattern>/sendNotificationServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
